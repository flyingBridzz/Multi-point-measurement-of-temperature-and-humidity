/* USER CODE BEGIN 1 */
void Delay_us_ON(void)
{
	HAL_TIM_Base_Start_IT(&htim6);
}

void Delay_us_OFF(void)
{
	HAL_TIM_Base_Stop_IT(&htim6);
}

uint8_t Delay_us_tim6(uint16_t x_us)
{
	Delay_us_Error = 0;
	Delay_us_Switch = 1;
	TIM6->CNT = 0;
	while((x_us > TIM6->CNT)&&(Delay_us_Error == 0));
	Delay_us_Switch = 1;
	return Delay_us_Error;
}

void HAL_TIM_PeriodElapsedCallback(TIM_HandleTypeDef *htim)
{
	 /* Prevent unused argument(s) compilation warning */
  //UNUSED(htim);
  /* NOTE : This function Should not be modified, when the callback is needed,
            the __HAL_TIM_PeriodElapsedCallback could be implemented in the user file
   */
	if((htim->Instance == TIM6)&&(Delay_us_Switch == 1))
	{
		Delay_us_Error = 1;
	}
	if(htim->Instance == TIM3)
	{
		cnt1ms++;
		cnt5ms++;
		cnt10ms++;
		cnt50ms++;
		cnt500ms++;
		cnt100ms++;
		cnt1000ms++;
		cnt2000ms++;
	}
}
